{
  "name": "@hometownjs/vite-config-vue",
  "version": "1.0.5",
  "description": "HoMeTownJS's vite config presets for Vue",
  "author": {
    "name": "HoMeTownJS",
    "email": "hometownsocool@gmail.com",
    "url": "https://github.com/HoMeTownJS"
  },
  "license": "MIT",
  "homepage": "https://github.com/HoMeTownJS/hometown-vite-config",
  "repository": {
    "url": "https://github.com/HoMeTownJS/hometown-vite-config.git"
  },
  "bugs": {
    "url": "https://github.com/HoMeTownJS/hometown-vite-config/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --config=index.js",
    "publish-pkg": "pnpm publish --access public",
    "update-version": "bumpp package.json"
  },
  "peerDependencies": {
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "unplugin-vue-components": "^0.24.1",
    "vite-plugin-html": "^3.2.0"
  },
  "devDependencies": {
    "@chialab/esbuild-plugin-meta-url": "^0.17.3",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "bumpp": "^9.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "tsup": "^6.5.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.0",
    "vite-plugin-html": "^3.2.0"
  }
}
